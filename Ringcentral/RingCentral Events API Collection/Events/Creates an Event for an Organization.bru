        "color": "string",
        "currency": "string",
        "description": "string",
        "doorsCloseAfterMinutes": 0,
        "doorsOpenBeforeMinutes": 0,
        "eventType": "hidden_event",
        "gdprText": "string",
        "hidePeopleArea": true,
        "message": "string",
        "name": "string",
        "networkType": "everyone",
        "password": "pa$$word",
        "shortDescription": "string",
        "slug": "string",
        "suppressEmails": true,
        "timeEnd": "2019-08-24T14:15:22Z",
        "timeStart": "2019-08-24T14:15:22Z",
        "timerLength": 0,
        "timezone": "Abu Dhabi"
      }
    }
  }
}

settings {
  encodeUrl: true
}
