meta {
  name: Creates an Event for an Organization
  type: http
  seq: 4
}

post {
  url: {{RingCentralEventsProdURL}}/v1/organizations/{{organizationId}}/events
  body: json
  auth: bearer
}

auth:bearer {
  token: {{bearer_token}}
}

body:json {
  {
    "data": {
      "type": "event",
      "attributes": {
        "analyticsCode": "string",
